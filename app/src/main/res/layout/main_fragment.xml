<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                             xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
                                             android:layout_height="match_parent">

    <TextView
            android:text="MainFragment"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView7" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="176dp" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="355dp" android:visibility="invisible"/>
    <Button
            android:text="Make Food"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/MakeFood"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="173dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="63dp"/>
    <Button
            android:text="Make Science"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/MakeScience" android:layout_marginTop="40dp"
            app:layout_constraintTop_toBottomOf="@+id/FoodVal" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="63dp"/>
    <TextView
            android:text="..."
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/FoodVal" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="63dp" android:layout_marginTop="11dp"
            app:layout_constraintTop_toBottomOf="@+id/MakeFood"/>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/ScienceVal" android:layout_marginTop="16dp"
            app:layout_constraintTop_toBottomOf="@+id/MakeScience" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="71dp" android:text="scienceString"/>
</android.support.constraint.ConstraintLayout>